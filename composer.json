{
  "name": "twfahey1/aqto_upstream",
  "description": "Defines common dependencies for Aqto projects.",
  "type": "framework",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "",
      "role": ""
    }
  ],
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  },
  "require": {
    "php": ">=8.3",
    "composer/installers": "^1.9",
    "cweagans/composer-patches": "^1.7",
    "drupal/admin_toolbar": "^3.3",
    "drupal/breadcrumbs_visibility": "^2.0",
    "drupal/coffee": "^2.0",
    "drupal/commerce": "^3.3",
    "drupal/commerce_view_receipt": "^2.0",
    "drupal/core-composer-scaffold": "^11",
    "drupal/core-recommended": "^11",
    "drupal/dropzonejs": "^2.8",
    "drupal/features": "^3.16",
    "drupal/field_permissions": "^1.2",
    "drupal/google_tag": "^2.0",
    "drupal/layout_builder_iframe_modal": "^1.3",
    "drupal/layout_builder_restrictions": "^3.0",
    "drupal/layout_builder_styles": "^2.0",
    "drupal/masquerade": "^2.0@RC",
    "drupal/metatag": "^2.2",
    "drupal/node_view_permissions": "^2.0",
    "drupal/page_title_visibility": "^2.0",
    "drupal/paragraphs": "^1.15",
    "drupal/paragraphs_entity_embed": "^4.0@beta",
    "drupal/pathauto": "^1.11",
    "drupal/symfony_mailer": "^1.2",
    "drupal/turnstile": "^1.1",
    "drupal/webform": "^6.3@beta",
    "drupal/webp": "^1.0@RC",
    "drupal/xmlsitemap": "^2.0",
    "drush/drush": "^13.0",
    "twfahey1/aqto_ai_codegen": "dev-main",
    "twfahey1/aqto_ai_core": "dev-main",
    "twfahey1/aqto_ajax_loader": "dev-main",
    "twfahey1/aqto_blocks": "dev-main",
    "twfahey1/aqto_defaults": "dev-main",
    "twfahey1/aqto_layout_builder_custom_sections": "dev-main",
    "twfahey1/aqto_layout_builder_styles": "dev-main",
    "twfahey1/aqto_page": "dev-main",
    "twfahey1/aqto_static": "dev-main",
    "twfahey1/aqto_text_formats": "dev-main",
    "twfahey1/aqto_theme_base": "dev-main",
    "vlucas/phpdotenv": "^5.1",
    "webflo/drupal-finder": "^1.2"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "drupal/core-composer-scaffold": true
    },
    "platform": {
      "php": "8.3.0"
    }
  },
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "web/"
      }
    },
    "patches": {
      "drupal/core": {
        "https://www.drupal.org/project/drupal/issues/3294680": "https://www.drupal.org/files/issues/2022-07-07/3294680-2.patch"
      }
    },
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "upstream-config": [
        "type:framework"
      ]
    }
  }
}
